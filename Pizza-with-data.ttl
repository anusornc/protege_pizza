#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
swrla:isRuleEnabled rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#hasBase
pizza:hasBase rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf pizza:hasIngredient ;
              owl:inverseOf pizza:isBaseOf ;
              rdfs:domain pizza:Pizza ;
              rdfs:range pizza:PizzaBase .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#hasIngredient
pizza:hasIngredient rdf:type owl:ObjectProperty ;
                    owl:inverseOf pizza:isIngredientOf .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#hasSpiciness
pizza:hasSpiciness rdf:type owl:ObjectProperty ;
                   rdfs:domain pizza:PizzaTopping ;
                   rdfs:range pizza:Spiciness .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#hasSpicinessPreference
pizza:hasSpicinessPreference rdf:type owl:ObjectProperty ;
                             rdfs:domain pizza:Customer ;
                             rdfs:range pizza:Spiciness .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#hasTopping
pizza:hasTopping rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf pizza:hasIngredient ;
                 owl:inverseOf pizza:isToppingOf ;
                 rdfs:domain pizza:Pizza ;
                 rdfs:range pizza:PizzaTopping .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#isBaseOf
pizza:isBaseOf rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf pizza:isIngredientOf .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#isIngredientOf
pizza:isIngredientOf rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#isMilderThan
pizza:isMilderThan rdf:type owl:ObjectProperty ;
                   owl:inverseOf pizza:isSpicierThan .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#isSpicierThan
pizza:isSpicierThan rdf:type owl:ObjectProperty ,
                             owl:TransitiveProperty ;
                    rdfs:domain pizza:Spiciness ;
                    rdfs:range pizza:Spiciness .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#isToppingOf
pizza:isToppingOf rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf pizza:isIngredientOf .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#purchasedByCustomer
pizza:purchasedByCustomer rdf:type owl:ObjectProperty ;
                          owl:inverseOf pizza:purchasedPizza ;
                          rdf:type owl:FunctionalProperty .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#purchasedPizza
pizza:purchasedPizza rdf:type owl:ObjectProperty ;
                     rdfs:domain pizza:Customer ;
                     rdfs:range pizza:Pizza .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#hasCaloricContent
pizza:hasCaloricContent rdf:type owl:DatatypeProperty ;
                        rdfs:domain pizza:Pizza ;
                        rdfs:range xsd:integer .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#hasDiscount
pizza:hasDiscount rdf:type owl:DatatypeProperty ;
                  rdfs:domain pizza:Customer ;
                  rdfs:range xsd:decimal .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#hasPhone
pizza:hasPhone rdf:type owl:DatatypeProperty ;
               rdfs:domain pizza:Person ;
               rdfs:range xsd:string .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#numberOfPizzasPurchased
pizza:numberOfPizzasPurchased rdf:type owl:DatatypeProperty ;
                              rdfs:domain pizza:Customer ;
                              rdfs:range xsd:integer .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#ssn
pizza:ssn rdf:type owl:DatatypeProperty ;
          rdfs:domain pizza:Employee ;
          rdfs:range xsd:string .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#AmericanaHotPizza
pizza:AmericanaHotPizza rdf:type owl:Class ;
                        rdfs:subClassOf pizza:NamedPizza ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty pizza:hasTopping ;
                                          owl:someValuesFrom pizza:JalapenoPepperTopping
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty pizza:hasTopping ;
                                          owl:someValuesFrom pizza:MozzarellaTopping
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty pizza:hasTopping ;
                                          owl:someValuesFrom pizza:PepperoniTopping
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty pizza:hasTopping ;
                                          owl:someValuesFrom pizza:TomatoTopping
                                        ] ;
                        rdfs:comment "A pizza that only has Mozzarella, Tomato, Jalapeno, and Pepperoni toppings" .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#AmericanaPizza
pizza:AmericanaPizza rdf:type owl:Class ;
                     rdfs:subClassOf pizza:NamedPizza ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty pizza:hasTopping ;
                                       owl:someValuesFrom pizza:MozzarellaTopping
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty pizza:hasTopping ;
                                       owl:someValuesFrom pizza:PepperoniTopping
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty pizza:hasTopping ;
                                       owl:someValuesFrom pizza:TomatoTopping
                                     ] ;
                     rdfs:comment "A pizza that only has Mozzarella, Tomato and Pepperoni toppings" .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#AnchovyTopping
pizza:AnchovyTopping rdf:type owl:Class ;
                     rdfs:subClassOf pizza:SeafoodTopping .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#CaperTopping
pizza:CaperTopping rdf:type owl:Class ;
                   rdfs:subClassOf pizza:VegetableTopping ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty pizza:hasSpiciness ;
                                     owl:hasValue pizza:Mild
                                   ] .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#CheeseTopping
pizza:CheeseTopping rdf:type owl:Class ;
                    rdfs:subClassOf pizza:PizzaTopping ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty pizza:hasSpiciness ;
                                      owl:hasValue pizza:Mild
                                    ] .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#CheesyPizza
pizza:CheesyPizza rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( pizza:Pizza
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty pizza:hasTopping ;
                                                               owl:someValuesFrom pizza:CheeseTopping
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#Customer
pizza:Customer rdf:type owl:Class ;
               owl:equivalentClass [ owl:intersectionOf ( pizza:Person
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty pizza:purchasedPizza ;
                                                            owl:someValuesFrom pizza:Pizza
                                                          ]
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty pizza:hasPhone ;
                                                            owl:someValuesFrom xsd:string
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#DeepPanBase
pizza:DeepPanBase rdf:type owl:Class ;
                  rdfs:subClassOf pizza:PizzaBase ;
                  owl:disjointWith pizza:ThinAndCrispyBase .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#Employee
pizza:Employee rdf:type owl:Class ;
               rdfs:subClassOf [ owl:intersectionOf ( pizza:Person
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty pizza:ssn ;
                                                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                        owl:onDataRange xsd:string
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#GreenPepperTopping
pizza:GreenPepperTopping rdf:type owl:Class ;
                         rdfs:subClassOf pizza:PepperTopping ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty pizza:hasSpiciness ;
                                           owl:hasValue pizza:Medium
                                         ] .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#HamTopping
pizza:HamTopping rdf:type owl:Class ;
                 rdfs:subClassOf pizza:MeatTopping ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty pizza:hasSpiciness ;
                                   owl:hasValue pizza:Mild
                                 ] .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#HighCaloriePizza
pizza:HighCaloriePizza rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( pizza:Pizza
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty pizza:hasCaloricContent ;
                                                                    owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                         owl:onDatatype xsd:integer ;
                                                                                         owl:withRestrictions ( [ xsd:minInclusive 400
                                                                                                                ]
                                                                                                              )
                                                                                       ]
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#HotVeggiePizza
pizza:HotVeggiePizza rdf:type owl:Class ;
                     owl:equivalentClass [ owl:intersectionOf ( pizza:VegetarianPizza
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty pizza:hasTopping ;
                                                                  owl:someValuesFrom [ rdf:type owl:Restriction ;
                                                                                       owl:onProperty pizza:hasSpiciness ;
                                                                                       owl:hasValue pizza:Hot
                                                                                     ]
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#InterestingPizza
pizza:InterestingPizza rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( pizza:Pizza
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty pizza:hasTopping ;
                                                                    owl:minQualifiedCardinality "3"^^xsd:nonNegativeInteger ;
                                                                    owl:onClass pizza:PizzaTopping
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#JalapenoPepperTopping
pizza:JalapenoPepperTopping rdf:type owl:Class ;
                            rdfs:subClassOf pizza:PepperTopping ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty pizza:hasSpiciness ;
                                              owl:hasValue pizza:Hot
                                            ] .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#LowCaloriePizza
pizza:LowCaloriePizza rdf:type owl:Class ;
                      owl:equivalentClass [ owl:intersectionOf ( pizza:Pizza
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty pizza:hasCaloricContent ;
                                                                   owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                        owl:onDatatype xsd:integer ;
                                                                                        owl:withRestrictions ( [ xsd:maxExclusive 400
                                                                                                               ]
                                                                                                             )
                                                                                      ]
                                                                 ]
                                                               ) ;
                                            rdf:type owl:Class
                                          ] .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#MargheritaPizza
pizza:MargheritaPizza rdf:type owl:Class ;
                      rdfs:subClassOf pizza:NamedPizza ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty pizza:hasTopping ;
                                        owl:someValuesFrom pizza:MozzarellaTopping
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty pizza:hasTopping ;
                                        owl:someValuesFrom pizza:TomatoTopping
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty pizza:hasTopping ;
                                        owl:allValuesFrom [ rdf:type owl:Class ;
                                                            owl:unionOf ( pizza:MozzarellaTopping
                                                                          pizza:TomatoTopping
                                                                        )
                                                          ]
                                      ] ;
                      rdfs:comment "A pizza that only has Mozzarella and Tomato toppings" .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#MeatTopping
pizza:MeatTopping rdf:type owl:Class ;
                  rdfs:subClassOf pizza:PizzaTopping .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#MediumVeggiePizza
pizza:MediumVeggiePizza rdf:type owl:Class ;
                        owl:equivalentClass [ owl:intersectionOf ( pizza:VegetarianPizza
                                                                   [ rdf:type owl:Restriction ;
                                                                     owl:onProperty pizza:hasTopping ;
                                                                     owl:allValuesFrom [ rdf:type owl:Restriction ;
                                                                                         owl:onProperty pizza:hasSpiciness ;
                                                                                         owl:someValuesFrom [ rdf:type owl:Restriction ;
                                                                                                              owl:onProperty pizza:isMilderThan ;
                                                                                                              owl:hasValue pizza:Hot
                                                                                                            ]
                                                                                       ]
                                                                   ]
                                                                 ) ;
                                              rdf:type owl:Class
                                            ] .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#MozzarellaTopping
pizza:MozzarellaTopping rdf:type owl:Class ;
                        rdfs:subClassOf pizza:CheeseTopping ;
                        owl:disjointWith pizza:ParmesanTopping .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#MushroomTopping
pizza:MushroomTopping rdf:type owl:Class ;
                      rdfs:subClassOf pizza:VegetableTopping ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty pizza:hasSpiciness ;
                                        owl:hasValue pizza:Mild
                                      ] .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#NamedPizza
pizza:NamedPizza rdf:type owl:Class ;
                 rdfs:subClassOf pizza:Pizza .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#OliveTopping
pizza:OliveTopping rdf:type owl:Class ;
                   rdfs:subClassOf pizza:VegetableTopping ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty pizza:hasSpiciness ;
                                     owl:hasValue pizza:Mild
                                   ] .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#ParmesanTopping
pizza:ParmesanTopping rdf:type owl:Class ;
                      rdfs:subClassOf pizza:CheeseTopping .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#PepperTopping
pizza:PepperTopping rdf:type owl:Class ;
                    rdfs:subClassOf pizza:VegetableTopping .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#PepperoniTopping
pizza:PepperoniTopping rdf:type owl:Class ;
                       rdfs:subClassOf pizza:MeatTopping ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty pizza:hasSpiciness ;
                                         owl:hasValue pizza:Medium
                                       ] .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#Person
pizza:Person rdf:type owl:Class .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#Pizza
pizza:Pizza rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty pizza:hasBase ;
                              owl:someValuesFrom pizza:PizzaBase
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty pizza:hasCaloricContent ;
                              owl:someValuesFrom xsd:integer
                            ] .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#PizzaBase
pizza:PizzaBase rdf:type owl:Class .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#PizzaTopping
pizza:PizzaTopping rdf:type owl:Class .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#PrawnTopping
pizza:PrawnTopping rdf:type owl:Class ;
                   rdfs:subClassOf pizza:SeafoodTopping .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#RedPepperTopping
pizza:RedPepperTopping rdf:type owl:Class ;
                       rdfs:subClassOf pizza:PepperTopping ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty pizza:hasSpiciness ;
                                         owl:hasValue pizza:Hot
                                       ] .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#SalamiTopping
pizza:SalamiTopping rdf:type owl:Class ;
                    rdfs:subClassOf pizza:MeatTopping ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty pizza:hasSpiciness ;
                                      owl:hasValue pizza:Medium
                                    ] .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#SeafoodTopping
pizza:SeafoodTopping rdf:type owl:Class ;
                     rdfs:subClassOf pizza:PizzaTopping ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty pizza:hasSpiciness ;
                                       owl:hasValue pizza:Mild
                                     ] .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#SohoPizza
pizza:SohoPizza rdf:type owl:Class ;
                rdfs:subClassOf pizza:NamedPizza ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty pizza:hasTopping ;
                                  owl:someValuesFrom pizza:MozzarellaTopping
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty pizza:hasTopping ;
                                  owl:someValuesFrom pizza:OliveTopping
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty pizza:hasTopping ;
                                  owl:someValuesFrom pizza:ParmesanTopping
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty pizza:hasTopping ;
                                  owl:someValuesFrom pizza:TomatoTopping
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty pizza:hasTopping ;
                                  owl:allValuesFrom [ rdf:type owl:Class ;
                                                      owl:unionOf ( pizza:MozzarellaTopping
                                                                    pizza:OliveTopping
                                                                    pizza:ParmesanTopping
                                                                    pizza:TomatoTopping
                                                                  )
                                                    ]
                                ] ;
                rdfs:comment "A pizza that  has Mozzarella, Olives, Parmesan, and Tomato toppings" .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#Spiciness
pizza:Spiciness rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Class ;
                                      owl:oneOf ( pizza:Hot
                                                  pizza:Medium
                                                  pizza:Mild
                                                )
                                    ] .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#SpicyBeefTopping
pizza:SpicyBeefTopping rdf:type owl:Class ;
                       rdfs:subClassOf pizza:MeatTopping ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty pizza:hasSpiciness ;
                                         owl:hasValue pizza:Hot
                                       ] .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#SpicyPizza
pizza:SpicyPizza rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( pizza:Pizza
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty pizza:hasTopping ;
                                                              owl:someValuesFrom [ rdf:type owl:Restriction ;
                                                                                   owl:onProperty pizza:hasSpiciness ;
                                                                                   owl:hasValue pizza:Hot
                                                                                 ]
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#ThinAndCrispyBase
pizza:ThinAndCrispyBase rdf:type owl:Class ;
                        rdfs:subClassOf pizza:PizzaBase .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#TomatoTopping
pizza:TomatoTopping rdf:type owl:Class ;
                    rdfs:subClassOf pizza:VegetableTopping .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#TunaTopping
pizza:TunaTopping rdf:type owl:Class ;
                  rdfs:subClassOf pizza:SeafoodTopping .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#VegetableTopping
pizza:VegetableTopping rdf:type owl:Class ;
                       rdfs:subClassOf pizza:PizzaTopping .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#VegetarianPizza
pizza:VegetarianPizza rdf:type owl:Class ;
                      owl:equivalentClass [ owl:intersectionOf ( pizza:Pizza
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty pizza:hasTopping ;
                                                                   owl:allValuesFrom [ rdf:type owl:Class ;
                                                                                       owl:unionOf ( pizza:CheeseTopping
                                                                                                     pizza:VegetableTopping
                                                                                                   )
                                                                                     ]
                                                                 ]
                                                               ) ;
                                            rdf:type owl:Class
                                          ] .


#################################################################
#    Individuals
#################################################################

###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#AmericanaHotPizza1
pizza:AmericanaHotPizza1 rdf:type owl:NamedIndividual ,
                                  pizza:AmericanaHotPizza ;
                         pizza:hasCaloricContent 767 .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#AmericanaHotPizza2
pizza:AmericanaHotPizza2 rdf:type owl:NamedIndividual ,
                                  pizza:AmericanaHotPizza ;
                         pizza:hasCaloricContent 675 .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#AmericanaHotPizza3
pizza:AmericanaHotPizza3 rdf:type owl:NamedIndividual ,
                                  pizza:AmericanaHotPizza .


###  http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#AmericanaPizza1
